        </activity>
        <activity
            android:name=".ClassicalActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".ArcadeActivity"
            android:label="@string/app_name" >
        </activity>
        <activity android:name=".HallOfFame" >
        </activity>
    </application>

</manifest>
